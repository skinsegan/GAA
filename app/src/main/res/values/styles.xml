<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Primary theme color of the app (sets background color of app bar) -->
        <item name="colorPrimary">#43A047</item>
        <!-- Background color of buttons in the app -->
        <item name="colorButtonNormal">#1b5122</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">400dp</item>
        <item name="android:background">@android:color/darker_gray</item>
    </style>
</resources>